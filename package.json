{
  "name": "@fsjsd/design-system",
  "version": "0.18.0",
  "main": "dist/exports.js",
  "module": "dist/exports.js",
  "types": "dist/exports.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "start": "react-scripts start",
    "build": "babel src --delete-dir-on-start --out-dir dist --extensions .ts,.tsx --presets=@babel/react,@babel/preset-typescript --ignore __tests__,spec.js,test.js,stories.js,stories.ts,stories.tsx,__snapshots__ && tsc --project tsconfig.types.json",
    "build-cra": "react-scripts build",
    "build-static": "react-scripts build && react-snapshot",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@types/jest": "26.0.22",
    "@types/node": "14.17.3",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-router-dom": "^5.1.2",
    "@types/styled-components": "5.1.9",
    "polished": "^4.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.0.0",
    "react-photo-gallery": "^8.0.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.3",
    "react-snapshot": "^1.3.0",
    "react-swipeable": "^6.0.0",
    "stackblur-canvas": "^2.2.0",
    "storybook": "^6.0.0",
    "styled-components": "5.2.3",
    "typescript": "4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@storybook/addon-actions": "6.2.8",
    "@storybook/addon-links": "6.2.8",
    "@storybook/addons": "6.2.8",
    "@storybook/react": "6.2.8",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "1.12.0"
  }
}
